csv_file = ARG1
latency_upper_bound = ARG2
thpt_lower_bound = ARG3
thpt_upper_bound = ARG4
set datafile separator comma
set y2tics thpt_lower_bound,thpt_upper_bound
set ytics nomirror
set y2range[thpt_lower_bound:thpt_upper_bound]
plot csv_file u 1:($3 < latency_upper_bound ? $3 : 1/0) axis x1y1 title "Publish latency", csv_file u 1:($4 < latency_upper_bound ? $4 : 1/0) axis x1y1 title "End to End latency", csv_file u 1:($5 > thpt_lower_bound && $5 < thpt_upper_bound ? $5 : 1/0) with lines axis x1y2 title "Sender throughput", csv_file u 1:($6 > thpt_lower_bound && $6 < thpt_upper_bound ? $6 : 1/0) with lines axis x1y2 title "Receiver throughput"
pause -1
